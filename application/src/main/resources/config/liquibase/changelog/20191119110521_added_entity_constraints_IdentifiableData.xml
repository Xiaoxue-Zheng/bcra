<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity IdentifiableData.
    -->
    <changeSet id="20191119110521-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="participant_id"
                                 baseTableName="identifiable_data"
                                 constraintName="fk_identifiable_data_participant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="participant"/>

        <addForeignKeyConstraint baseColumnNames="csv_file_id"
                                 baseTableName="identifiable_data"
                                 constraintName="fk_identifiable_data_csv_file_id"
                                 referencedColumnNames="id"
                                 referencedTableName="csv_file"/>

    </changeSet>
</databaseChangeLog>
